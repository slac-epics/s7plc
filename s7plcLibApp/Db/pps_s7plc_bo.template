record(bo, "$(dtype):$(area):$(position):$(attribute)") {
  field(DESC, "$(description)")
  field(DTYP, "S7plc")
  field(OUT,  "@$(plc)/$(byte_offset) T=$(data_type) B=$(bit)")
  field(ZNAM, "$(zname)")
  field(ONAM, "$(oname)")
  field(ZSV,  "$(zsv)")
  field(OSV,  "$(osv)")
  field(PINI, "YES")
}
